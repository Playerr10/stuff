	&quot;http://tadah.rocks/asset?id=4099&quot;,
	&quot;http://tadah.rocks/asset?id=4100&quot;
}

local cooldown = false
local cooldownTime = 7

-- Functions
script.Parent.Activated:connect(function()
	if not cooldown and not activated.IsPlaying then
		cooldown = true		
		
		sound.SoundId = tabletSounds[math.random(1,#tabletSounds)]
		sound:Play()
		
		wait(cooldownTime)
		cooldown = false
		sound:Stop()
	end
end)</ProtectedString>
