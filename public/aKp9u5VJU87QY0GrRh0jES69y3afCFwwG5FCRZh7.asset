--FUNCTIONS--
local function FindIndexOfValue(Table, Value)
	for i, CurrentValue in pairs(Table) do
		if CurrentValue == Value then
			return i
		end
	end
end

local function Round(Number, NumberDecimalPlaces)
	local Multiplier = 10^(NumberDecimalPlaces or 0)
	return math.floor(Number * Multiplier + 0.5) / Multiplier
end

local function GenerateRandomInt(Min, Max)
     return Round(Min + math.random() * (Max - Min))   
end

local function DecideAscii()
	local Ranges = {
		-- NUMBERS
		{
			Min = 48,
			Max = 57
		},
		
		-- UPPERCASE
		{
			Min = 65,
			Max = 90
		},
		
		-- LOWERCASE
		{ 
			Min = 97,
			Max = 122
		}
	}

	local Range = Ranges[GenerateRandomInt(1, #Ranges)]
	local Character = GenerateRandomInt(Range.Min, Range.Max)

	return string.char(Character)
end

local function MakeId()
	local IdString = &quot;&quot;

	for i = 1, 6 do
		IdString = IdString .. DecideAscii()
	end

	return IdString
end

local function MoneyTimer(Part, Time)
	Spawn(function()
		wait(Time)
		
		if Part:FindFirstChild(&quot;PartId&quot;) then
			if FindIndexOfValue(_G.ReignParts, Part.PartId.Value) then
				table.remove(_G.ReignParts, FindIndexOfValue(_G.ReignParts, Part.PartId.Value))
			end
		end

		Part:Remove()
	end)
end

local function MakeBasePart()
	local Part = Instance.new(&quot;Part&quot;)
	Part.Size = Size

	Part.Position = Vector3.new((GenerateRandomInt(-25, 25)),30,(GenerateRandomInt(-25,25)))
	
	Part.TopSurface = 0
	Part.BottomSurface = 0
	Part.Anchored = false
	Part.Locked = true
	Part.Shape = Shape
	Part.formFactor = FormFactor

	return Part
end

local function MakeValue(Part, GiveType, Amount)
