					<ProtectedString name="Source">local chat = game:GetService(&quot;Chat&quot;)
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local keyDown

local sound = script.Parent.Handle.Sound
local activated = script.Parent.Handle.Activated

local function FindFirstChildOfClass(object, className)
	for i, thing in pairs(object:GetChildren()) do
		if thing:IsA(className) then
			return thing
		end
	end
	
	return nil
end

script.Parent.Activated:connect(function()
	local target = mouse.Target
	
	local humanoid = FindFirstChildOfClass(target.Parent, &quot;Humanoid&quot;)
	if humanoid then
		target:BreakJoints()
		humanoid.Health = 0
	end
end)

local function processKeyDown(key)
	print(&quot;Key:&quot;, key, &quot; Code:&quot;, string.byte(key))
	
	if key == &quot;p&quot; then
		if player.Character then
			local ff = player.Character:FindFirstChild(&quot;ForceField&quot;)
			if ff then
				ff:remove()
			else
				if not activated.IsPlaying and not sound.IsPlaying then
					activated:Play()
				end
				
				Instance.new(&quot;ForceField&quot;, player.Character)
				chat:Chat(script.Parent.Handle, &quot;ForceField!&quot;, Enum.ChatColor.Red)
				
				if activated.IsPlaying then
					wait(5)
					activated:Stop()
				end
			end
		end
	end
	
	if key == &quot;l&quot; then
		if player.Character then
			local humanoid = player.Character:FindFirstChild(&quot;Humanoid&quot;)
			if humanoid then
				if humanoid.WalkSpeed &gt; 16 then
					humanoid.WalkSpeed = 16
				else
					if not activated.IsPlaying and not sound.IsPlaying then
						activated:Play()
					end
					
					humanoid.WalkSpeed = 50
					chat:Chat(script.Parent.Handle, &quot;Speed!&quot;, Enum.ChatColor.Red)
					
					if activated.IsPlaying then
						wait(5)
						activated:Stop()
					end
				end
			end
		end
	end
	
	if key == &apos;m&apos; then
		if player.Backpack then
			if not activated.IsPlaying and not sound.IsPlaying then
				activated:Play()
			end	
			
			Instance.new(&quot;HopperBin&quot;, player.Backpack).BinType = Enum.BinType.Grab
			Instance.new(&quot;HopperBin&quot;, player.Backpack).BinType = Enum.BinType.Clone
			Instance.new(&quot;HopperBin&quot;, player.Backpack).BinType = Enum.BinType.Hammer
			chat:Chat(script.Parent.Handle, &quot;Build tools!&quot;, Enum.ChatColor.Red)
			
			if activated.IsPlaying then
				wait(5)
				activated:Stop()
			end
		end
	end
end

script.Parent.Equipped:connect(function()
	keyDown = mouse.KeyDown:connect(processKeyDown)
end)
script.Parent.Unequipped:connect(function()
	keyDown:disconnect()
end)</ProtectedString>
