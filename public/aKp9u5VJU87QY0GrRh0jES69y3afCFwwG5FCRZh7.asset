					<ProtectedString name="Source">-- Services

-- Variables
local sound = script.Parent.Handle.Sound
local activated = script.Parent.Handle.Activated

local tabletSounds = {
	&quot;http://tadah.rocks/asset?id=4094&quot;,
	&quot;http://tadah.rocks/asset?id=4095&quot;,
