function fire(aim)
	--sp.Handle.Fire:Play()

t=r.Stepped:wait()
	last6=last5
	last5=last4
	last4=last3
	last3=last2
	last2=last
	last=t
	partHit=&quot;nil&quot;
	typeHit=nil
	dmgHit=nil
	headshot=false

	local bullet=Bullet:clone()
	local totalDist=0
	Lengthdist=-RayLength/.5
	local startpoint=sp.Handle.CFrame*BarrlePos
	local dir=(aim)-startpoint
	local cfrm=CFrame.new(startpoint, dir+startpoint)
	local hit,pos,normal,time=raycast(game.Workspace, startpoint, cfrm*Vector3.new(0,0,Lengthdist)-startpoint, function(brick)
		if brick.Name==&quot;Glass&quot; then
			return true
		elseif brick.Name==&quot;Bullet&quot; or brick.Name==&quot;BulletTexture&quot; then
			return false
		elseif brick:IsDescendantOf(sp.Parent) then
			return false
		elseif brick.Parent.className==&quot;Hat&quot; and brick.Parent:IsDescendantOf(player.Character)==false then
			brick:destroy()
			return false
		elseif brick.Name==&quot;Handle&quot; then
			if brick.Parent:IsDescendantOf(sp.Parent) then
				return false
			else
				return true
			end
		end
		return true
	end)
	bullet.Parent=game.Workspace
	if hit~=nil then
		distance=(startpoint-pos).magnitude
		local humanoid=hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
		if humanoid~=nil then
			print(humanoid.Health)
			print(humanoid.MaxHealth)
			local num = math.random(1,4)--same as: math.random(1, #fallAnims)
			local num2 = math.random(1,2)--same as: math.random(1, #fallAnims)
			local selection = &quot;Hit &quot; .. num
			local selection2 = &quot;Shield Break New &quot; .. num2
			if humanoid.Health&gt;100 then
				player.PlayerGui.WeaponGui.WeaponFrame[&quot;Shield &quot; .. selection]:play()
			elseif humanoid.Health&lt;=100 then
				player.PlayerGui.WeaponGui.WeaponFrame[selection]:play()
			end
			local damage=BaseDamage-((distance/RayLength) * 15)
			if damage &lt; MinDamage then damage=MinDamage end
			if hit.Name==&quot;Head&quot; then
				player.PlayerGui.WeaponGui.WeaponFrame.Crit:play()
				damage=damage*2
				partHit=hit
				headshot=true
				if humanoid.MaxHealth&gt;=101 then
					typeHit=3
				elseif humanoid.MaxHealth&lt;=100 then
					typeHit=1
				end
				dmgHit=math.floor(damage+.5)
			else
				damage=damage
				partHit=hit
				if humanoid.MaxHealth&gt;=101 then
					typeHit=4
				elseif humanoid.MaxHealth&lt;=100 then
					typeHit=2
				end
				dmgHit=math.floor(damage+.5)
			end
			tagHumanoid(humanoid,headshot)
			if humanoid.Health&gt;0 then
				local eplr=game.Players:playerFromCharacter(humanoid.Parent)
				local plr=game.Players:playerFromCharacter(sp.Parent)
				if eplr~=nil and plr~=nil then
				--	if eplr.TeamColor~=plr.TeamColor or eplr.Neutral or plr.Neutral then
						tagHumanoid(humanoid,headshot)
						-- humanoid:TakeDamage(damage)
						humanoid.Health = humanoid.Health - damage -- for testing :)
						if humanoid.Health&lt;=100 and humanoid.MaxHealth&gt;=101 then
							hitIndicator(partHit,5,0,tostring(humanoid.Parent),game.Players:playerFromCharacter(sp.Parent),true)
							player.PlayerGui.WeaponGui.WeaponFrame[&quot;Shield Break&quot;]:play()
							player.PlayerGui.WeaponGui.WeaponFrame[selection2]:play()
						elseif humanoid.Health==0 then
							hitIndicator(partHit,6,0,tostring(humanoid.Parent),game.Players:playerFromCharacter(sp.Parent),true)
							killedPlayer(tostring(humanoid.Parent),headshot)
						end
				--	end
