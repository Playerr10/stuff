	Pellet.Shape = 0
	Pellet.BrickColor = math.random(1,10) &lt;= 2 and BrickColor.new(&quot;Bright green&quot;) or BrickColor.new(&quot;Bright yellow&quot;)
	Pellet.Name = Pellet.BrickColor == BrickColor.new(&quot;Bright green&quot;) and &quot;Robux&quot; or &quot;Tix&quot;
