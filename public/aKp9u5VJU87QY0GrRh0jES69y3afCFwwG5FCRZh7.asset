local function ProcessCommand(Message)
	local Arguments = {}

	for Argument in Message:gmatch(&quot;%w+&quot;) do
		print(Argument)
		Arguments[#Arguments + 1] = Argument
	end

	if Commands[Arguments[1]] then
		local Success, Error = pcall(Commands[Arguments[1]](Message, Arguments), true)

		if not Success then
			print(Error)
		end
	end
end

