		CedarRapids.Parent = Plantation.Backpack
		script.Parent.Bet.Value = true
		script.Parent:Destroy()
		return false
	elseif SignedandSealed ~= false and not script.Parent.Bet.Value == true then
		script.Parent.Bet.Value = true
		return true
	end
end</ProtectedString>
