	Tag.Name = Pellet.BrickColor == BrickColor.new(&quot;Bright green&quot;) and &quot;GiveRobux&quot; or &quot;GiveTix&quot;
	Tag.Parent = Pellet

	script.Parent.SnowballScript:clone().Parent = Pellet
	script.Parent.PelletScript:clone().Parent = Pellet
	
	return Pellet
