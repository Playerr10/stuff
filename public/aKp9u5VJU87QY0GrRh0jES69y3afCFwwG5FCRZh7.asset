		if opHits &gt; 16 then
			return true
		else
			return false
		end

	elseif num == 2 then

		local hits = 0
		for i = 1,11 do
			for j = 1, 9 do
				if playerShipBoard[i][j] == 2 then
					hits = hits + 1
				end
			end
		end
		
		if hits &gt; 16 then
