local tool = script.Parent
local rf = script.Parent.Best
local debounce = false
local ins = game:GetService(&quot;InsertService&quot;)
local Debris = game:GetService(&quot;Debris&quot;)

function Beam(owner)
	for i = 1, math.random(600,700) do
		local la = ins:LoadAsset(4069):GetChildren()[1]
		local newbeam = la:Clone()
		newbeam.Parent = workspace
		newbeam.Position = Vector3.new(owner.Torso.Position.X + math.random(-240,240), owner.Torso.Position.Y + math.random(240,360), owner.Torso.Position.Z + math.random(-240,240))
		newbeam.Rotation = Vector3.new(math.random(0,359),math.random(0,359),math.random(0,359))
		Debris:AddItem(newbeam, 8)
		wait()
	end
end

tool.Activated:connect(function()
	local owner = tool.Parent
	if debounce or not script.Parent.Bet.Value == true then return end
	debounce = true
	local coolmusic = classical:Clone()
	coolmusic.Parent = workspace
	coolmusic.Volume = 0.4
	coolmusic.PlayOnRemove = true
	coolmusic:Destroy()
	tool.Enabled = false
	wait(11)
	Beam(owner)
	wait(5)
	for i,v in pairs(workspace:GetChildren()) do
		if v.Name == &quot;Beam&quot; then
			v:Destroy()
		end
	end
	tool.Enabled = true
	debounce = false
end)

rf.OnServerInvoke = function(Plantation, SignedandSealed)
	if SignedandSealed ~= true and not script.Parent.Bet.Value == true then
